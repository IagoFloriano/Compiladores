program cmdIf (input, output);
var i, j: integer;       
begin                   
   i:=0;                   
   while (i < j) do        
   begin                   
      if (i div 2 * 2 = i)  
        then   i := 0
        else   i := 1;
      i := i+1                    
   end                    
end. 

